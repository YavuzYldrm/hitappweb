{
    "name": "hitappengine-web",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": "10.x"
    },
    "scripts": {
        "start": "nodemon ./bin/www",
        "dev": "concurrently -k true --kill-others-on-fail true -s all \"npm:watch\" \"npm:start\"",
        "build": "parcel build ./public/recommendation/scripts/main.js -d ./public/recommendation/scripts/ --no-source-maps --out-file bundle.js",
        "watch": "parcel watch ./public/recommendation/scripts/main.js -d ./public/recommendation/scripts/ --out-file bundle.js",
        "sync": "browser-sync start --proxy localhost:3000 --files ./public/recommendation/templates",
        "test": "mocha --timeout 10000"
    },
    "dependencies": {
        "angular": "1.7.8",
        "angular-cookies": "^1.7.8",
        "angular-route": "^1.7.8",
        "bootstrap": "^4.3.1",
        "compression": "^1.7.4",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.0.0",
        "express": "~4.16.1",
        "flatpickr": "^4.6.1",
        "http-errors": "~1.6.3",
        "jquery": "^3.4.1",
        "ng-resource": "^1.3.2",
        "nodemon": "^1.19.1",
        "popper.js": "^1.14.7",
        "serve-favicon": "^2.5.0"
    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "concurrently": "^4.1.1",
        "mocha": "^6.1.4",
        "parcel-bundler": "^1.12.3"
    },
    "alias": {
        "parcel": "parcel",
        "nodemon": "nodemon",
        "browser-sync": "browser-sync"
    }
}
